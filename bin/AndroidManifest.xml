<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.game1"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="19" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.game1.MainActivity"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.game1.StartActivity"
            android:label="@string/title_activity_stat" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.game1.PlayActivity"
            android:label="@string/title_activity_play" >
        </activity>
        <activity
            android:name="com.example.game1.EasyActivity"
            android:label="@string/title_activity_easy" >
        </activity>
        <activity
            android:name="com.example.game1.NomalActivity"
            android:label="@string/title_activity_nomal" >
        </activity>
        <activity
            android:name="com.example.game1.HardActivity"
            android:label="@string/title_activity_hard" >
        </activity>
        <activity
            android:name="com.example.game1.GoalActivity"
            android:label="@string/title_activity_goal" >
        </activity>
    </application>

</manifest>
